{
  "name": "devapt-core-doc",
  "version": "2.0.0",
  "description": "Developpers Application Toolkit - core documentation builder",
  "author": "Luc BORIES <luc.bories@gmail.com> (https://github.com/lucbories)",
  "license": "Apache-2.0",
  "repository": "git+https://github.com/lucbories/devapt-core-doc.git",
  "bugs": {
    "url": "git+https://github.com/lucbories/devapt-core-doc/issues"
  },
  "homepage": "https://github.com/lucbories/devapt-core-doc#readme.md",
  "main": "index.html",
  "engines": {
    "node": ">=4.4.4"
  },
  "directories": {
    "dist": "./dist"
  },
  "scripts": {
    "build_manuals": "esdoc -c ./.esdoc-manuals.json > ./tmp/esdoc-manuals.out",
    "build_common": "esdoc -c ./.esdoc-common.json > ./tmp/esdoc-common.out",
    "build_server": "esdoc -c ./.esdoc-server.json > ./tmp/esdoc-server.out"
  },
  "keywords": [
    "server",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "redux",
    "baconjs",
    "reactive",
    "devtools"
  ],
  "dependencies": {
    "devapt-core-browser": ">=2.0.0",
    "devapt-core-common": ">=2.0.0",
    "devapt-core-server": ">=2.0.0",
    "devapt-core-services": ">=2.0.0",
    "cheerio": "^0.22.0",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "del": ">=2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-load-plugins": ">=1.2.0",
    "parallelshell": "^2.0.0"
  }
}
